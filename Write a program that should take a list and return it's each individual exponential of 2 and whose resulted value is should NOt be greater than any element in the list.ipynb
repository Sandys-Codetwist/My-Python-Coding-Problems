    "def exp_num(l):\n",
    "    l1=[2**i for i in l]\n",
    "    l2=[]\n",
    "    for i in l1:\n",
    "        if i<max(l):\n",
    "            l2.append(i)\n",
    "            return l2\n",
    "exp_num([1,2,3,4,5]) "
